class MyStack {

    // 1.  Create a stack
   int arr[] = new int[1000];
   int top = -1;
   
    
    public void push(int x) {
        // code here
        if(top < 1000){
            top++;
            arr[top] = x;
        }
        
    }

    public int pop() {
        
        if(top > -1){
            int x = arr[top];
            top--;
            return x;
        }
        return -1;
    }
}
